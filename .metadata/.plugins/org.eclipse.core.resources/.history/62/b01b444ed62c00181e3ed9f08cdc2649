package com.cts;

import java.time.LocalDate;
import java.time.LocalTime;
import java.time.Month;
public class LocalDateTime {
	
	


		public static void main(String[] args) {
			
			
			
			//Creating LocalDateTime by providing input arguments
			LocalDate specificDate = LocalDate.of(2017, Month.FEBRUARY, 10);
			System.out.println("Specific Date="+specificDate);
			LocalDate sixtyfifth2017 = LocalDate.ofYearDay(2017, 65);
		    System.out.println(sixtyfifth2017);
		    LocalTime fromSecondsOfDay = LocalTime.ofSecondOfDay(12345);
		    System.out.println(fromSecondsOfDay);
		    LocalTime specificTime = LocalTime.of(12,20,25,12:00);
			System.out.println("Specific Time of Day="+specificTime);
		    
		}
			
				
		}


